;\r\n\r\n    useEffect(() => {\r\n        if (props.account) { \r\n        axios.get(`http://localhost:4000/home/${localStorage.getItem(\"sessionID\").toString()}/saves`)\r\n            .then(res => setSavedArray(res.data.savedPets))\r\n            .catch(err => console.log(err))\r\n        }\r\n    }, [])\r\n\r\n    useEffect(() => {\r\n        if (props.account) { \r\n        axios.get(`http://localhost:4000/home/${localStorage.getItem(\"sessionID\").toString()}/adopts`)\r\n            .then(res => setAdoptedArray(res.data.fosteringPets))\r\n            .catch(err => console.log(err))\r\n        }\r\n    })\r\n\r\n    const selectedPet = (pet) => {\r\n        setCurrentPet(pet);\r\n        // history.push(`${history.location.pathname}/${pet._id}`)\r\n        setModalOpen(true);\r\n    }\r\n\r\n    const savePet = (pet) => {\r\n        if (props.account) { \r\n        axios.put(`http://localhost:4000/home/${localStorage.getItem(\"sessionID\").toString()}/saved/${pet._id}`)\r\n            .then(res => setSavedArray(res.data.savedPets))\r\n            .catch(err => console.log(err));\r\n        }\r\n    }\r\n\r\n    const unSavePet = (pet) => {\r\n        if (props.account) { \r\n        axios.delete(`http://localhost:4000/home/${localStorage.getItem(\"sessionID\").toString()}/saved/${pet._id}`)\r\n            .then(res => setSavedArray(res.data.savedPets))\r\n            .catch(err => console.log(err));\r\n        }\r\n    }\r\n\r\n    const adoptPet = (pet) => {\r\n        if (props.account === true) { \r\n        axios.put(`http://localhost:4000/home/${localStorage.getItem(\"sessionID\").toString()}/adopts/${pet._id}`)\r\n            .then(res => setAdoptedArray(res.data.fosteringPets))\r\n            .catch(err => console.log(err));\r\n        }\r\n    }\r\n\r\n    const returnPet = (pet) => {\r\n        if (props.account === true) { \r\n        axios.delete(`http://localhost:4000/home/${localStorage.getItem(\"sessionID\").toString()}/adopts/${pet._id}`)\r\n     