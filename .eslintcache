dArray.includes(`${pet._id}`) ? <FontAwesomeIcon color=\"red\" style={{ cursor: \"pointer\" }} onClick={() => savePet(pet)} className=\"mr-2 awesome-icon\" icon={regularHeart}></FontAwesomeIcon> : <FontAwesomeIcon color=\"red\" onClick={() => unSavePet(pet)} className=\" awesome-icon\" icon={solidHeart}></FontAwesomeIcon>}\r\n                                </CardFooter>\r\n                                :\r\n                                null}\r\n                        </Card>\r\n                    )\r\n                })}\r\n            </Row>\r\n            <PetModal account={props.account} savePet={props.account ? (content) => savePet(content) : null} unSavePet={props.account ? (content) => unSavePet(content) : null} saves={props.account ? savedArray : null} adoptPet={props.account ? (content) => adoptPet(content) : null} returnPet={props.account ? (content) => returnPet(content) : null} adopts={props.account ? adoptedArray : null} pet={currentPet} myImage={currentPet.image} openMe={modalOpen} setOpenMe={(bool) => setModalOpen(bool)} />\r\n        </Container>\r\n    )\r\n}\r\n\r\nexport default PetCards;","C:\\Users\\joey-\\OneDrive\\Desktop\\Spijer-Pet Adoption Project\\pet-adoption-front-end-jsjow\\src\\Loginpage\\LoginPage.js",[],"C:\\Users\\joey-\\OneDrive\\Desktop\\Spijer-Pet Adoption Project\\pet-adoption-front-end-jsjow\\src\\NavBar\\Navigation.js",[],"C:\\Users\\joey-\\OneDrive\\Desktop\\Spijer-Pet Adoption Project\\pet-adoption-front-end-jsjow\\src\\Homepage\\Home.js",[],"C:\\Users\\joey-\\OneDrive\\Desktop\\Spijer-Pet Adoption Project\\pet-adoption-front-end-jsjow\\src\\Loginpage\\SignUp.js",["185","186"],"C:\\Users\\joey-\\OneDrive\\Desktop\\Spijer-Pet Adoption Project\\pet-adoption-front-end-jsjow\\src\\Admin\\AdminDashboard.js",["187","188","189","190","191"],"C:\\Users\\joey-\\OneDrive\\Desktop\\Spijer-Pet Adoption Project\\pet-adoption-front-end-jsjow\\src\\SearchPage\\SimpleSearch.js",["192"],"import React, { useState } from 'react';\r\nimp